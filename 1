#include <stdio.h>

void insertion(int list[], int n){
  int i,j,next;
  for(i = 1; i < n; i++)
  {
    next = list[i];
    for( j = i-1; j>=0 && next < list[j]; j--)
    {
      list[j+1] = list[j];

    }
    list[j+1]= next;
  }

}

int main(){
    int list [10]={2,3,1,4,5,7,8,9,10,6};
    int n = 10;

    insertion(list, n);

    for(int i=0; i<n; i++)
      printf("%d ", list[i]);

  
    return 0;

}
